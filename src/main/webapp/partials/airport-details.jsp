<div class="container">
	<div class="page-header">
		<h2 align="center">
			<i class="fa fa-info"></i> Airport Details
		</h2>
	</div>
	<div class="panel panel-primary">
		<div class="panel-body">
			<div class="col-md-12">
				<div class="project-desc-box">
					<h3>{{airport.name}}</h3>
					<dl class="dl-horizontal">
						<dt>
							<h5>Airport ID</h5>
						</dt>
						<dd>
							<h5>{{airport.id}}</h5>
						</dd>
						<dt>
							<h5>Airport Code</h5>
						</dt>
						<dd>
							<h5>{{airport.code}}</h5>
						</dd>
						<dt>
							<h5>Latitude</h5>
						</dt>
						<dd>
							<h5>{{airport.lat}}</h5>
						</dd>
						<dt>
							<h5>Longitude</h5>
						</dt>
						<dd>
							<h5>{{airport.lon}}</h5>
						</dd>
						<dt>
							<h5>Rating</h5>
						</dt>
						<dd>
							<h5>{{airport.rating}}</h5>
						</dd>
						<dt>
							<h5>City</h5>
						</dt>
						<dd>
							<h5>{{airport.city}}</h5>
						</dd>
						<dt>
							<h5>State</h5>
						</dt>
						<dd>
							<h5>{{airport.state}}</h5>
						</dd>
						<dt>
							<h5>State</h5>
						</dt>
						<dd>
							<h5>{{airport.country}}</h5>
						</dd>
						<dt>
							<h5>Url</h5>
						</dt>
						<dd>
							<h5><a href="{{airport.url}}">{{airport.url}}</a></h5>
						</dd>
						<dt>
							<h5>Elevation</h5>
						</dt>
						<dd>
							<h5>{{airport.elev}}</h5>
						</dd>
						<dt>
							<h5>Direct Flight</h5>
						</dt>
						<dd>
							<h5>{{airport.directFlight}}</h5>
						</dd>
					</dl>
				</div>
			</div>
		</div>
	</div>
</div>













